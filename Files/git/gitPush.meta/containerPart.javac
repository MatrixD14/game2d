    if (file == null) return; //[I-S]LN=28;[I-E]
    for (File f : file) { //[I-S]LN=29;[I-E]
      if (f.isDirectory()) busca(f, resul); //[I-S]LN=30;[I-E]
      else resul.add(f); //[I-S]LN=31;[I-E]
    } //[I-S]LN=32;[I-E]
  } //[I-S]LN=33;[I-E]
 //[I-S]LN=34;[I-E]
