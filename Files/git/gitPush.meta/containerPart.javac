    busca(dir, resul); //[I-S]LN=37;[I-E]
    return resul.toArray(new File[0]); //[I-S]LN=38;[I-E]
  } //[I-S]LN=39;[I-E]
 //[I-S]LN=40;[I-E]
  private void processFile() { //[I-S]LN=41;[I-E]
